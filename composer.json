{
  "name": "lbausch/fortress",
  "description": "Roles and Permissions for Laravel's Authorization",
  "license": "MIT",
  "keywords": [
    "laravel",
    "acl",
    "rbac",
    "roles",
    "permissions"
  ],
  "support": {
    "issues": "https://github.com/lbausch/fortress/issues",
    "source": "https://github.com/lbausch/fortress"
  },
  "authors": [
    {
      "name": "Lorenz Bausch",
      "email": "info@lorenzbausch.de"
    }
  ],
  "autoload": {
    "psr-4": {
      "Bausch\\Fortress\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "require": {
    "laravel/framework": "7.*"
  },
  "require-dev": {
    "phpunit/phpunit": "8.*",
    "orchestra/testbench": "^5.2"
  },
  "minimum-stability": "stable",
  "extra": {
    "laravel": {
        "providers": [
            "Bausch\\Fortress\\FortressServiceProvider"
        ]
    }
  }
}

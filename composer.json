{
  "name": "lbausch/laravel-fortress",
  "description": "Roles and Permissions for Laravel's Authorization",
  "license": "MIT",
  "keywords": [
    "laravel",
    "acl",
    "rbac",
    "roles",
    "permissions"
  ],
  "authors": [
    {
      "name": "Lorenz Bausch",
      "email": "info@lorenzbausch.de"
    }
  ],
  "autoload": {
    "psr-4": {
      "Bausch\\LaravelFortress\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Bausch\\LaravelFortress\\Tests\\": "tests/"
    }
  },
  "require": {
    "laravel/framework": "5.2.*|5.3.*|5.4.*"
  },
  "require-dev": {
    "phpunit/phpunit": "5.*"
  },
  "minimum-stability": "stable"
}
